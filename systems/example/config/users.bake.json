[
  {
    "resource_module": "users",
    "resource": {
      "identifier": "users",
      "type": "users",
      "icon": "user",
      "layouts": {
        "detail": "${file('/config/users.detail.json')}"
      },
      "fields": [
        {
          "identifier": "name",
          "type": "text"
        },
        {
          "identifier": "email",
          "type": "email"
        },
        {
          "identifier": "password",
          "type": "password",
          "options": {
            "ignore_when_empty": true
          }
        },
        {
          "identifier": "roles",
          "type": "multiselect",
          "options": {
            "references": "roles"
          }
        }
      ]
    }
  }
]
